{% extends "base.njk" %}

{% set templateName = "post" %}

{% block precontent %}
<aside id="toc-container">
    <h2>Contents</h2>
    {{ content | toc }}
</aside>
{% endblock %}


{% block prefooter %}
	<div id="taglist">
		<h6>Tags: </h6>
        <ul>
			{%- for tag in tags | filterTagList -%}
			    <li><a href="{{ site.site_url }}/tag/{{ tag | slug }}">{{ tag }}</a></li>
			{%- endfor -%}
        </ul>
	</div>
{% endblock %}
